  <script>
    // Template filtering functionality
    document.addEventListener('DOMContentLoaded', function() {
      const categoryButtons = document.querySelectorAll('.template-category-btn');
      const templates = document.querySelectorAll('.template-card');

      categoryButtons.forEach((button: Element) => {
        button.addEventListener('click', function(this: HTMLElement, event: Event) {
          const category = this.getAttribute('data-category');

          // Update active button
          categoryButtons.forEach((btn: Element) => {
            const buttonElement = btn as HTMLElement;
            buttonElement.classList.remove('bg-blue-600', 'text-white');
            buttonElement.classList.add('bg-gray-200', 'text-gray-700');
          });

          this.classList.remove('bg-gray-200', 'text-gray-700');
          this.classList.add('bg-blue-600', 'text-white');

          // Filter templates
          templates.forEach((template: Element) => {
            const templateElement = template as HTMLElement;
            if (category === 'all' || template.getAttribute('data-category') === category) {
              templateElement.style.display = 'block';
            } else {
              templateElement.style.display = 'none';
            }
          });
        });
      });

      // Load more functionality
      const loadMoreBtn = document.getElementById('load-more-templates') as HTMLButtonElement | null;
      let currentVisible = 20;

      if (loadMoreBtn) {
        loadMoreBtn.addEventListener('click', function(this: HTMLButtonElement, event: Event) {
          // In a real implementation, this would load more templates from a database/API
          // For now, we'll just show a message
          this.textContent = 'Loading...';
          setTimeout(() => {
            this.textContent = 'More templates coming soon!';
            this.disabled = true;
            this.classList.add('opacity-50', 'cursor-not-allowed');
          }, 1000);
        });
      }
    });
  </script>
